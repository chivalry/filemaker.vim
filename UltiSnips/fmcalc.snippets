# Python library

global !p
def complete( t, opts ):
	if t:
#   The next five lines are for reference, translating the following line into
#		a standard for loop.
#		non_t = []
#		for m in opts:
#			if m.startswith(t):
#				non_t.append(m[len(t):])
#		opts = non_t
		opts = [ m[len(t):] for m in opts if m.lower().startswith(t.lower()) ]
		
	if len(opts) == 1:
		return opts[0]

	return '(' + '|'.join(opts) + ')'
endglobal

# --------------------------------------
global !p
# A single place to store global setting names and their default values.

# The setting for placing a space between a function call and its opening
#   parenthsis. By default functions are of the form `Func( param )`. If
#   ADD_SPACE_AFTER_FUNCTION is true, functions take the form of
#   `Func ( param )`.
ADD_SPACE_AFTER_FUNCTION		= 'g:FMVAddSpaceAfterFunction'
ADD_SPACE_DEFAULT						= False

# The setting for placing a space between a function's parenses and its
#   first and last parameter. By default functions are of the form
#   `Func( param1 )`. If PAD_SPACES_WITHIN_PARENS is false, functions take
#   the form `Func(param1)`.
PAD_SPACES_WITHIN_PARENS		= 'g:FMVPadSpacesWitinParens'
PAD_SPACES_DEFAULT					= True

# The setting and default for placing each parameter of a multi-parameter function on its own line. By default mult-parameter functions are of the form:
#    Func(
#      param1;
#      param2
#    )
# If SEPARATE_PARAMS_WITH_SPACE is true, functions take the form of:
#    Func( param1; param2 )
SEPARATE_PARAMS_WITH_SPACE	= 'g:FMVSeparateParamsWithSpace'
SEPARATE_PARAMS_DEFAULT			= True

# The setting and default for inserting `Get( FileName )` as a function parameter
#   when appropriate. By default such functions are of the form
#   `Func( Get( FileName ) )`. If FILL_IN_GET_FILENAME is false, such functions
#   take the form `Func( fileName )`.
FILL_IN_GET_FILENAME				= 'g:FMVFillInGetFileName'
FILL_GET_DEFAULT						= True
endglobal

# --------------------------------------
global !p
# Return a space or empty string based on the user's settings.
def add_space():
	if snip.opt( ADD_SPACE_AFTER_FUNCTION, ADD_SPACE_DEFAULT ):
		return ' '
	else:
		return ''
endglobal

# --------------------------------------
global !p
# Return a space or empty string based on the user's settings.
def pad_spaces():
	if snip.opt( PAD_SPACES_WITHIN_PARENS, PAD_SPACES_DEFAULT ):
		return ' '
	else:
		return ''
endglobal

# --------------------------------------
global !p
# Returns the format of a function's opening parenthsis based on user settings.
def open_paren():
	return add_space() + '(' + pad_spaces()
endglobal

# --------------------------------------
global !p
# Returns the format of a function's closing parenthsis based on user settings.
def close_paren():
	return pad_spaces() + ')'
endglobal

# --------------------------------------
global !p
# Returns the format of a multi-parameter function's opening parenthsis based on
#   user settings.
def first_separator( sub_function = False ):
	if snip.opt( PAD_SPACES_WITHIN_PARENS, PAD_SPACES_DEFAULT ):
		space = ' '
	else:
		space = ''
	
	if snip.opt( SEPARATE_PARAMS_WITH_SPACE, SEPARATE_PARAMS_DEFAULT ):
		return '(' + space
	else:
		if sub_function:
			return '(' + '\n\t\t'
		else:
			return '(' + '\n\t'
endglobal

# --------------------------------------
global !p
# Returns the format of a multi-parameter function's closing parenthsis based on
#   user settings.
def last_separator( sub_function = False ):
	if snip.opt( PAD_SPACES_WITHIN_PARENS, PAD_SPACES_DEFAULT ):
		space = ' '
	else:
		space = ''
	
	if snip.opt( SEPARATE_PARAMS_WITH_SPACE, SEPARATE_PARAMS_DEFAULT ):
		return space + ')'
	else:
		if sub_function:
			return '\n\t' + ')'
		else:
			return '\n' + ')'
endglobal

# --------------------------------------
global !p
# Returns the format of a multi-parameter function's spacing between parameters
#   based on user settings.
def param_separator( sub_function = False ):
	if snip.opt( SEPARATE_PARAMS_WITH_SPACE, SEPARATE_PARAMS_DEFAULT ):
		return ' '
	else:
		if sub_function:
			return '\n\t\t'
		else:
			return '\n\t'
endglobal

# --------------------------------------
global !p
# Returns the appropriate parameter of functions that could take `Get( FileName )`
#   as a parameter, based on user settings.
def get_filename():
	if snip.opt( FILL_IN_GET_FILENAME, FILL_GET_DEFAULT ):
		open_paren = add_space() + '(' + pad_spaces()
		close_paren = pad_spaces() + ')'
		return 'Get' + open_paren + 'FileName' + close_paren
	else:
		return 'fileName'
endglobal

